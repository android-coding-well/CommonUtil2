<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gosuncn.cu">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.main.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module.databinding.DataBindingActivity"
            android:label="DataBinding" />
        <activity
            android:name=".module.greendao.activity.GreenDaoActivity"
            android:label="greenDAO" />
        <activity android:name=".module.permission.activity.PermissionActivity" />
        <activity android:name=".module.dagger2.Dagger2Activity" />
        <activity android:name=".module.annotation.AnnotationActivity" />
        <activity android:name=".module.retrofit.RetrofitActivity" />
        <activity android:name=".module.rxjava.RxjavaActivity" />
        <activity android:name=".module.image.ImageActivity" />
        <activity android:name=".module.animview.AnimViewActivity" />
        <activity android:name=".module.other.TestActivity" />
        <activity android:name=".module.other.OtherActivity"></activity>
    </application>

</manifest>